{
  "name": "pro-el-components",
  "type": "module",
  "version": "0.0.5",
  "description": "A Advanced Component Library based on ElementPlus",
  "author": "mingNeo",
  "license": "MIT",
  "homepage": "https://mingNeo.github.io/pro-el-components/",
  "repository": {
    "type": "git",
    "url": "https://github.com/mingNeo/pro-el-components.git"
  },
  "bugs": {
    "url": "https://github.com/mingNeo/pro-el-components/issues"
  },
  "keywords": [
    "pro-el-components",
    "component library",
    "ui framework",
    "ui",
    "vue"
  ],
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/pro-el-components.es.js",
      "require": "./dist/lib/pro-el-components.es.js",
      "default": "./dist/lib/pro-el-components.es.js"
    },
    "./dist/index.css": "./dist/lib/style.css",
    "./dist/style.css": "./dist/lib/style.css",
    "./style.css": "./dist/lib/style.css",
    "./dist/lib/style.css": "./dist/lib/style.css",
    "./es/components/*": {
      "types": "./dist/es/components/*/index.d.ts",
      "import": "./dist/es/components/*/index.js"
    },
    "./es/components/*/style.css": "./dist/es/components/*/style.css",
    "./components/*": {
      "types": "./dist/es/components/*/index.d.ts",
      "import": "./dist/es/components/*/index.js"
    },
    "./components/*/style.css": "./dist/es/components/*/style.css",
    "./es": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/pro-el-components.es.js"
    },
    "./dist/es/*": {
      "types": "./dist/es/components/*/index.d.ts",
      "import": "./dist/es/components/*/index.js"
    },
    "./resolver": {
      "types": "./dist/resolver/resolver.d.ts",
      "import": "./dist/resolver/resolver.es.js",
      "require": "./dist/resolver/resolver.js",
      "default": "./dist/resolver/resolver.es.js"
    },
    "./*": "./*"
  },
  "main": "./dist/lib/pro-el-components.es.js",
  "module": "./dist/lib/pro-el-components.es.js",
  "types": "./dist/lib/index.d.ts",
  "resolver": "./dist/resolver/resolver.js",
  "resolverTypes": "./dist/resolver/resolver.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  },
  "scripts": {
    "dev": "vite --port 3333 --open",
    "build:test": "vite build --mode test",
    "build:staging": "vite build --mode staging",
    "build": "npm run clean && pnpm run build:es && pnpm run build:components && pnpm run build:resolver",
    "build:components": "vite build --mode build-components",
    "build:resolver": "vite build --config vite.resolver.config.mts",
    "build:es": "vite build --mode production",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.json",
    "clean": "npx rimraf dist",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "@unocss/reset": "^66.0.0",
    "@vueuse/core": "^13.1.0",
    "@vueuse/head": "^2.0.0",
    "@vueuse/integrations": "^13.1.0",
    "axios": "^1.9.0",
    "element-plus": "^2.9.9",
    "husky": "^9.1.7",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "uuid": "^11.1.0",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-virtual-html": "^1.2.6",
    "vue": "^3.5.13",
    "vue-demi": "^0.14.10",
    "vue-i18n": "^10.0.7",
    "vue-matomo": "^4.2.0",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@microsoft/api-extractor": "^7.52.11",
    "@types/js-cookie": "^3.0.6",
    "@types/markdown-it-link-attributes": "^3.0.5",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.6",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "critters": "^0.0.25",
    "cross-env": "^7.0.3",
    "cypress": "^14.3.2",
    "eslint": "^9.25.1",
    "eslint-plugin-cypress": "^4.3.0",
    "express": "^5.1.0",
    "glob": "^10.3.10",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-shiki": "^0.9.0",
    "pnpm": "^10.10.0",
    "postcss-nesting": "^13.0.1",
    "rimraf": "^5.0.5",
    "shiki": "^3.3.0",
    "taze": "^19.0.4",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "unocss": "^66.0.0",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-dts": "1.0.0-beta.0",
    "unplugin-element-plus": "^0.10.0",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "1.4.0",
    "vue-tsc": "^2.2.10"
  }
}